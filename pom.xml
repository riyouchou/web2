        <nacos.client.version>2.2.0</nacos.client.version>
        <com.alibaba.druid.version>1.1.13</com.alibaba.druid.version>
        <mybatis.plus.version>3.5.5</mybatis.plus.version>
        <mybatis.dynamic.datasource>3.4.0</mybatis.dynamic.datasource>
        <mysql.connector.version>8.0.31</mysql.connector.version>
        <commons.pool2.version>2.11.1</commons.pool2.version>
        <okhttp.version>4.10.0</okhttp.version>
        <stripe-java.version>28.0.0</stripe-java.version>
        <itextpdf.version>7.1.15</itextpdf.version>
        <esignature.version>3.22.0</esignature.version>

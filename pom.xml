        <spring-boot-dependencies.version>2.7.18</spring-boot-dependencies.version>
        <spring-cloud-dependencies.version>2021.0.5</spring-cloud-dependencies.version>
        <lombok.version>1.18.30</lombok.version>
        <log4j.version>1.2.17</log4j.version>
        <fastjson.version>1.2.83</fastjson.version>
        <apache.commons-lang3.version>3.12.0</apache.commons-lang3.version>
        <apache.commons-io.version>2.11.0</apache.commons-io.version>
        <apache.commons-codec.version>1.16.0</apache.commons-codec.version>
        <apache.commons-text.version>1.10.0</apache.commons-text.version>
